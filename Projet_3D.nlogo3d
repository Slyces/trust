to setup
  clear-all
  reset-ticks
  ; â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€ DEFINITION CONSTANTES â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€ ;
  set service "quality"
  ;set range_max round (range_vision);
  set range_max round (range_vision_provider * (1 + coeff_radius_max))
  set H 10
  set alpha_min 0.25
  set alpha_max 1.0
  set p_I []
  set pC []
  set pW []
  set pT []


  ; ------------------------- Constantes pour trust -------------------------- ;
  set branching_factor 2
  set referral_length 5
  set lambda (- 5 / ln 0.5)
  set gamma (- ln 0.5)

  ; ---------------- Intervales de performance selon le type ----------------- ;
  set pl_perfect 10
  set pl_good 5
  set pl_ok 0
  set pl_bad -5
  set pl_worst -10

  ; ------------------------- Poids des composantes -------------------------- ;
  set W_R 2.0
  if Interaction = false [ set W_I 0.0 ]
  if Witness = false [ set W_W 0.0 ]
  if Certificate = false [ set W_C 0.0 ]

  ; â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€ FIN CONSTANTES â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€ ;

  ; ------------------------ Initialisation consumer ------------------------- ;
  create-consumers nbConsumer [ ; Projection sur la sphÃ¨re
    tilt-up asin (1.0 - random-float 2)
    jump range_circle
  ]
  ask consumers [
    set color blue
    set ratingData []
    set listProvider []
    set listConsumer []
    set need_update false
    ;set alpha 1
    set alpha random_uniform alpha_min alpha_max;
  ]
  ask consumers [ ; CrÃ©e la liste des voisins disponibles;
    ask consumers with [distance_sphere self myself <= range_vision_consumer][
      set listConsumer lput [who] of myself listConsumer
    ]
    set listConsumer remove [who] of self listConsumer
  ]
  ; ------------------------ Initialisation providers ------------------------ ;
  create-providers nbProvider [
    tilt-up asin (1.0 - random-float 2)
    jump range_circle
  ]
  ask providers [
    set type_provider 0
    set listCertificate []
    set max_certificate [0 0 -20 0]
    set need_update false

    ;donne l'id du provider aux consumers
    ask consumers with [distance_sphere self myself <= range_max] [
      set listProvider lput [who] of myself listProvider
    ]
  ]

  ; --------------------------- Types de providers --------------------------- ;
  ; Good
  ask n-of round (nbProvider / 10) providers with [type_provider = 0][
    set color green
    set type_provider "good"
    set mu random_uniform pl_good pl_perfect
    set sigma 1.0
  ]
  ; Odinary
  ask n-of round (nbProvider / 2.5) providers with [type_provider = 0][
    set color orange
    set type_provider "ordinary"
    set mu random_uniform pl_ok pl_good
    set sigma 1.0
  ]
  ; Bad
  ask n-of round (nbProvider / 20) providers[
    set color red
    set type_provider "bad"
    set mu random_uniform pl_worst pl_ok
    set sigma 2.0
  ]
  ; Intermitent
  ask n-of round (nbProvider / 2.22) providers with [type_provider = 0][
    set color violet
    set type_provider "intermittent"
  ]
end

to go
  ; â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€ Interactions â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€ ;
  set log_with_diffusion 0
  set nb_actif 0
  let listRandom [] ; Pour random
  let listNormal [] ;[> Pour IT;

  ; ---------------------- Transaction avec un provider ---------------------- ;
  ask consumers with [length listProvider != 0][
    set listRandom lput give_utility one-of listProvider listRandom
    if random-float 1 < alpha [
      let idProvider 0

      ifelse length listProvider != 1 [ ; Si plusieurs provider son dispo
        set idProvider selectProvider
      ][ set idProvider one-of listProvider ]

      makeTransaction idProvider

      set nb_actif nb_actif + 1
      if distance_sphere self turtle idprovider > range_vision_provider [
        set log_with_diffusion log_with_diffusion + 1
      ]

      set listNormal lput utility listNormal;
    ]
  ]
  set utilityRandom mean listRandom
  set utilityIt mean listNormal;
  ;set utilityIt mean [utility] of consumers with [length listProvider != 0]

  ; ------------------------ Gestion des certificats ------------------------- ;
  if Certificate [
    ask providers with [need_update][
      update_certificate
      set need_update false
      set max_certificate [0 0 -20 0]
    ]
  ]

  ; ----- Ajout de la transaction au consumer [problÃ¨mes de temps rÃ©el] ------ ;
  ask consumers with [need_update][
    set ratingData lput temp_transaction ratingData
    if length ratingData > H [
      set ratingData but-first ratingData
    ]
    set need_update false
  ]
  ifelse empty? p_I [ set m_PI 0 ][set m_PI mean p_I]
  ifelse empty? pW [ set m_PW 0 ][set m_PW mean pW]
  ifelse empty? pC [ set m_PC 0 ][set m_PC mean pC]
  ifelse empty? pT [ set m_PT 0 ][set m_PT mean pT]


  set p_I []
  set pW []
  set pC []
  set pT []

  tick
end

; â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€ Selection d'un provider â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€ ;
to-report selectProvider
  ; let trustValues []
  let selected 0
  let temperature ln (ticks  + 1)
  ;let temperature sqrt (ticks  + 1)

  ; added
  let list_not_trusted []
  let best_provider_id (-1)
  let best_trust_value (-2) ; valeur < min(trust_value)

  foreach listProvider [ ; Pour chaque provider du client
    idProvider ->
    let trust_value Trust idProvider
    ;set trustValues lput (list (Trust idProvider) idProvider) trustValues
    ifelse trust_value != 5 [
      if trust_value > best_trust_value [
        set best_provider_id idProvider
        set best_trust_value trust_value
      ]
    ][
      set list_not_trusted fput idProvider list_not_trusted
    ]
  ]

  if best_trust_value = (-2) [
    report one-of list_not_trusted
  ]
  if empty? list_not_trusted [
      report best_provider_id
  ]

  ; ------------ choix du provider en fonction de la temperature ------------- ;
  let err_profit exp ( best_trust_value * 10 * temperature)
  let err_explore exp ( utility_mean * temperature )

  let p_expl err_explore / ( err_explore + err_profit )
  ifelse random-float 1 < p_expl [
    set selected one-of list_not_trusted
  ][
    set selected best_provider_id
  ]
  report selected
end

; â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€ Calcul de la confiance â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€ ;

; ------------------------ Valeur de confiance finale ------------------------ ;
to-report Trust [idProvider]
  let wt 0
  let w 0

  if W_I > 0 [
    let tr_list_IT getTrListIT self idProvider
    let T_I trustFunction tr_list_IT "IT"
    set p_I lput item 1 T_I p_I
    set w w + (W_I * item 1 T_I)
    set wt wt + (W_I * item 1 T_I * item 0 T_I)
  ]

  if W_W > 0 [
    let tr_list_WR getTrListWR idProvider
    let T_W trustFunction tr_list_WR "WT"
    set pW lput item 1 T_W pW
    set w w + (W_W * item 1 T_W)
    set wt wt + (W_W * item 1 T_W * item 0 T_W)
  ]

  if W_C > 0 [
    let T_C trustFunction [listCertificate] of turtle idProvider "CT"
    set pC lput item 1 T_C pC
    set w w + (W_C * item 1 T_C)
    set wt wt + (W_C * item 1 T_C * item 0 T_C)
  ]

  if W_R > 0 []

  let rho_T w / (W_I + W_W + W_C + W_R)
  set pT lput rho_T pT
  if rho_T = 0 [ report 5 ]

  let T wt / w
  report T
end

; ------------------- Valeurs de confiances intermÃ©diaires ------------------- ;
to-report trustFunction [list_transaction component]
  let up 0
  let down 0
  let weight 0

  ifelse length list_transaction = 0 [ ; T_K
    report list 5 0
  ][
    foreach list_transaction [
      transaction ->
      set weight giveWeight transaction component
      set up (up + weight * item 2 transaction)
      set down (down + weight)
    ]
    let TK up / down

    let PRK (1 - exp (( - gamma) * down)) ; Ïð‘…ð¾

    set up 0
    set down 0
    set weight 0

    foreach list_transaction [ ; Ïð·ð¾
      transaction ->
      set weight giveWeight transaction component
      set down (down + weight)
      set up up + (weight * abs (item 2 transaction - TK))
    ]

    let PDK (1 - (0.5 * (up / down)))
    let PK PRK * PDK

    report list TK PK
  ]
end

; ---------------------- Poids d'un rating (fiabilitÃ©) ----------------------- ;
to-report giveWeight [transaction component]
  ifelse component = "RT" [ ; r = ...
  ][ ; r = (b, c, v, t)
    let time ticks - item 3 transaction
    report exp ( - (time / lambda))
  ]
end

