to-report getTrListIT [consumer_C id_provider]
  report filter [
    tr -> item 0 tr = id_provider and item 1 tr = service
  ] ratingData
end

; Wr
to-report getTrListWR [id_provider]
  let visited []
  let i 0

  let temp recurcive_transaction_W who id_provider visited i
  report temp
end

to-report recurcive_transaction_W [id_consumer id_provider visited compteur]
  let temp_listTransaction []
  let listTransaction []

  ifelse compteur < referral_length [
    set compteur compteur + 1
    let V best_neighbors id_consumer id_provider

    foreach V [ a ->    ;âˆ€ v âˆˆ V:
      if not member? a visited [  ;si v âˆ‰ visited:
        set visited fput a visited      ;visited.add(v)
        set temp_listTransaction (getTrListIT (turtle a) id_provider)      ;RI_v = R_I(v, b)
        if empty? temp_listTransaction [
          set temp_listTransaction recurcive_transaction_W a id_provider visited compteur
        ]
        set listTransaction sentence listTransaction temp_listTransaction ;merge(R, RI_v)
      ]
    ]
    report listTransaction
  ][ report [] ]
end

; Trie la liste des consumers en fonction de la distance avec le provider
to-report best_neighbors [idConsumer idProvider]
  ifelse empty? [listConsumer] of turtle idConsumer [
    report []
  ][

    let list_distance []
    let retour []
    foreach [listConsumer] of turtle idConsumer [ x ->
      set list_distance lput list x (distance_sphere turtle x turtle idProvider) list_distance
    ]
    set list_distance sort-by [ [a b] ->
      item 1 a < item 1 b
    ]list_distance
    set list_distance take branching_factor list_distance
    foreach list_distance [x ->
      set retour lput item 0 x retour
    ]
    report retour
  ]
end

to makeTransaction [id_provider]
  let temp give_utility id_provider

  ;normalize l'utilitÃ©
  let note utility / 10

  ;fait la moyenne de toutes les transactions dÃ©ja effÃ©ctuÃ©.
  set utility_sum utility + utility_sum
  set nbTransaction nbTransaction + 1
  set utility_mean utility_sum / nbTransaction

  let transaction (list id_provider service note ticks)

  set temp_transaction transaction
  set need_update true

  ;donne la note au provider
  if Certificate [
    ask turtle id_provider [
      if note > item 2 max_certificate[
        set max_certificate transaction
      ]
      set need_update true
    ]
  ]
end

to-report give_utility [id_provider]
  let distance_P 0
  let mean_P 0
  let type_P 0
  let sigma_P 0

  ;distance entre le consumer et le provider
  ask turtle id_provider [
    if range_max > range_vision_provider [ set distance_P distance_sphere self myself ]
    set mean_P mu
    set type_P type_provider
    set sigma_P sigma
  ]

  ifelse type_P = "intermittent" [
    ;random entre -5 et 5
    set utility (random-float 10) - 5
  ][
    set utility random-normal mean_P sigma_P
  ]

  ; ---------- dÃ©gradation linÃ©aire de l'utilitÃ© liÃ©e Ã  la distance ---------- ;
  if range_max > range_vision_provider [
    ;dÃ©gradation linÃ©aire de la distance
    let coeff (- 10 - utility) / (range_max - range_vision_provider)

    if distance_P > range_vision_provider [
      set utility utility + (coeff * (distance_P - range_vision_provider))
    ]
    if distance_P >= range_max [
      set utility -10
    ]
  ]
  if utility < -10 [ set utility -10 ]
  if utility > 10 [ set utility 10 ]
  report utility
end

to update_certificate
  ifelse length listCertificate < H [
    set listCertificate lput max_certificate listCertificate
  ][
    if item 2 max_certificate > item 2 first listCertificate [
      set listCertificate remove first listCertificate listCertificate
      set listCertificate lput max_certificate listCertificate
    ]
  ]
  set listCertificate sort-by [
    [a b] -> item 2 a < item 2 b] listCertificate
end

to-report take [n items]
  report sublist items 0 min list n (length items)
end

to-report random_uniform [min-extreme max-extreme]
  report random-float (max-extreme - min-extreme) + min-extreme
end

;to-report cached_dist [cons_id prov_id];
  ;set cons_id (cons_id - min-one-of consumers);
  ;set prov_id (cons_id - min-one-of providers);
;end;

to-report distance_sphere [a b]
  let temp ((([xcor] of a * [xcor] of b) + ([ycor] of a * [ycor] of b) + ([zcor] of a * [zcor] of b)) / (range_circle * range_circle))
  if temp > 1.0 [
    set temp 1.0
  ]

  report range_circle * acos (temp)
end
