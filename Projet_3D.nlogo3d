; Constantes
; | N        | nombre de rounds              | 50           |
; | H        | history size                  | 10           |
; | λ        | recency scaling factor        | - 5 / ln(.5) |

; | η_BF     | branching factor              | 2            |
; | η_RL     | referral max length           | 5            |

; | α_min    | espace de l'activity level    | 0.25         |
; | α_max    |                               | 1.00         |

; | ug_min   | utilité minimale              | -10          |
; | ug_max   | ---   maximale                | -10          |

; | r_cons   | r_o consumer                  | ??           |
; | r_prov   | r_o provider                  | ??           |

; | PL_perf  | palier perfect                | 10           |
; | PL_good  | --   good                     | 10           |
; | PL_ok    | --   ok                       | 10           |
; | PL_bad   | --   bad                      | 10           |
; | PL_worst | --   worst                    | 10           |

; | Np       | #Providers                    | 100          |
; | Npg      | - Good                        | 10           |
; | Npo      | - Ordinary                    | 40           |
; | Npb      | - Bad                         | 5            |
; | Npi      | - Intermitent                 | 45           |
; | Nc       | #Consumer                     | 500          |

; | W_I      | Poids Interactions            | 2.0          |
; | W_R      |  ---  Roles                   | 2.0          |
; | W_W      |  ---  Witnesses               | 2.0          |
; | W_C      |  ---  Certificats             | 2.0          |

; | γ_I      | constante gamma               | - ln(.5)     |
; | γ_R      |                               | - ln(.5)     |
; | γ_W      |                               | - ln(.5)     |
; | γ_C      |                               | - ln(.5)     |

; | p_ppc    | %max nouveaux provider        | 2%           |
; | p_cpc    | %max    --    consumer        | 5%           |

; | p_plp    | proba qu'un provider bouge    | 10%          |
; | p_plc    | ---  -- -- consumer  ---      | 10%          |
; | ΔΦ_p     | borne déplacement provider    |  π / 20      |
; | ΔΦ_c     | ---      ----    consumer     |  π / 20      |

; | p_μc     | proba de changement de μ      | 10%          |
; | M        | Δμ max                        | 1.0          |
; | p_pr_sw  | proba de changement de profil | 2%           |
;
; Fonctions utilitaires
;   - U(a, b) : flotant aléatoire entre a et b inclus
;   - G(μ, σ) : variable aléatoire suivant une loi normale N(μ, σ)
;   - d(a, b) : distance euclidienne entre a et b (où a = (ax, ay, az))
;               simplification depuis la distance à la surface d'une sphère [@H]
;   - r_choice(L) : retourne un élément aléatoire de L
;   - rand(p) : retourne vrai avec une probabilité p
;   - register(L, T) : enregistre le tuple T dans la liste L
;       si L n'est pas plein
;          ajouter T
;       sinon
;          supprimer l'entrée (a, b, v, t) avec t minimal
;   - register_best(L, T) : enregistre le tuple T s'il est meilleur que
;                           le moins bon tuple de L
;       si T(v) > min(T'(v)) ∀ T' ∈ L
;          ajouter T à la place de T' le min
;   - curr_t(void) : récupérer le round en cours
;   - t(r) : renvoie le round où un rating a été créé
;   - near_providers(a) : donne les providers dispo pour l'agent a [@H]
;       retourner les providers p tq d(a, p) < a.r_o
;   - near_consumers(a) : donne les providers dispo pour l'agent a
;       retourner les consumers c tq d(a, c) < a.r_o
;   - T(t) : 1 / t : température pour l'exploration de Boltzmann [@H]
;
;
; Initialisation Consumer
;   - (x, y, z) aléatoire uniforme à la surface de la sphère [@H]
;   - r_o = r_cons
;   - α = U(α_min, α_max)
;   - ug_sum = 0 : somme des ug gagnés    depuis le début
;   - ug_n = 0   : nombre d'intéractions  ------ -- -----
;   - ug_mean(void): renvoie l'ug moyen dur toutes les transactions
;       return ug_sum / ug_n
;   - L = liste de taille H contentant des éléments de forme
;       (a, b, r, t) : a = agent consumer
;                      b = agent provider
;                      r = valeur de l'interaction
;                      t = temps en round
;       simplification depuis (a, b, c, i, r) car c unique et i non-utilisé [@H]
;   - val(ug) : valuation de l'UG pour a
;        return ug / 10
;
; Initialisation Provider
;   - Type parmi : (good, ordinary, bad, intermitent)
;   - case(type) :
;      good     : μ = U(PL_good, PL_perf) [@H]
;                 σ = 1.0
;      ordinary : μ = U(PL_ok, PL_good)
;                 σ = 1.0
;      bad      : μ = U(PL_worst, PL_ok)
;                 σ = 2.0
;   - r_o = r_prov
;   - provide(void) : fonction
;      si type ∈ (good, ordinary, bad)
;          renvoie μ       (simplification de G(μ, σ) ) [@H]
;      sinon
;          renvoie U(PL_bad, PL_good)
;
; Interaction(a, b) : a consumer, b provider
;    ug = b.provide()   récupérer l'ug de b
;    a.ug_n += 1
;    a.ug_sum += ug
;    v = val(ug)
;    r = (a, b, v, current_round())
;    register(a.L, r)         | pseudocode, mais essayer de
;    register_best(b.L, r)    | prendre en compte la simultanéité
;
; Choix d'un provider : a consumer
;    P = near_providers(a)
;    T_max = -1
;    p_max = ∅
;    LT = liste vide
;    LNT = liste vide
;    ∀ p ∈ P
;       T_p = Trust(a, p)
;       si T_p ≠ ∅
;          add(LT, (p, T_p))
;          si T_p > T_max:
;              T_max = T_p
;              p_max = p
;       sinon
;          add(LNT, p)
;    err_profit  = exp( T_max / T(t) )
;    err_explore = exp( ug_mean() / T(t) )
;    p_expl = err_expl / (err_expl + err_profit)
;    if rand(p_expl) :
;        return r_choice(LNT)
;    else
;        return p_max
;
; @TODO : rechercher la valeur de T (Blotzmann exploration strategy)
;         on part sur du ( 1 / t )
;
; fonction ω_I, ω_W, ω_C: r un rating (a, b, c, i, v)
;   Δt(r) = curr_t - t(r)
;   return exp(- Δt / λ )
;
; function ω_R : r un rating (a, b, c, e, v)
;   return e
;
; Calcul de T_I, T_W, T_C: ensemble de rating R
;   Σω𝑣 = 0
;   Σω = 0
;   ∀ r ∈ R:
;       Σω𝑣 += ω_I(r) * r.𝑣
;       Σω = ω_I(r)
;   T_K = Σω𝑣 / Σω
;   ρ_RK = 1 - exp(-γ_K * Σω)
;   ∀ r ∈ R:
;       Σω𝑣𝑇 += ω_I(r) * |r.𝑣 - T_K|
;   ρ_DK = 1 - 0.5 * ( Σω𝑣𝑇  / Σω )
;   ρ_K = ρ_RK * ρ_DK
;   return (T_K, ρ_K)
;
; Construction de R_I: consumer a, provider b
;   return { r | r.b = b, t(r) < curr_t }
;
; Construction de R_W: consumer a, provider b
;   V = near_consumers(a)
;   sort(V, dist(x, b))
;   sélectionner les η_BF meilleurs dans V_best
;   ∀ v ∈ V_best:
;
;
;
;
;
;
; Construction de R_C: consumer a, provider b
;
; Construction de R_R: consumer a, provider b
;
;
;
; Dynamisme de population :
;   Δprov = U(0, p_ppc)
;   retirer (Δprov * Np) random providers
;   ajouter (Δprov * Np) random providers (de même type ?) [@H]
;                                         (de même distribution ?) [@H]
;   Δcons = U(0, p_cpc)
;   retirer (Δcons * Nc) random providers
;   ajouter (Δcons * Nc) random providers (de même type ?) [@H]
;                                         (de même distribution ?) [@H]
;   ∀ p ∈ population(providers):
;       if rand(p_ps):
;           changer le profil de p
;           réinitialiser μ_𝑝, σ_𝑝
;       else if rand(p_μ𝑐): [@H] (else if)
;           Δμ = U(-M, +M)
;           p.μ = max(p.μ_min, min(p.μ_max, p.μ + Δ_μ) ) [@H]
;       if rand(p_plc):
;           Δθ = U(−ΔΦ, +ΔΦ)
;           Δφ = U(−ΔΦ, +ΔΦ)
;           p.θ += Δθ
;           p.φ += Δφ
;   ∀ c ∈ population(consumers):
;       if rand(p_clc):
;           Δθ = U(−ΔΦ, +ΔΦ)
;           Δφ = U(−ΔΦ, +ΔΦ)
;           c.θ += Δθ
;           c.φ += Δφ
;
; nombre d'agent constant
; proportion des catégories des provider page 139 on peut tirer sur une loi normale
; est ce qu'on doit faire mourir et naitre des providers ?

breed [providers provider]
breed [consumers consumer]

consumers-own [
  listProvider
  ratingData   ;liste des transactions que le consumer a effectué max 10
  utility
  utility_sum
  utility_mean
  nbTransaction
]
providers-own [
  mean_provider
  type_provider
]



globals [
  idTransaction
  service
  utility-IT
  utility-RANDOM

  range_max
]



to setup
  clear-all
  reset-ticks
  set service "quality"
  set range_max round (range_provider + (range_provider * coeff_radius_max))

  create-consumers nbConsumer [
    tilt-up asin (1.0 - random-float 2)
    jump range_circle
  ]

    create-providers nbProvider [
    tilt-up asin (1.0 - random-float 2)
    jump range_circle
  ]


    ask consumers [
    set color red
    set ratingData []
    set listProvider []
  ]

  ask providers [
    set color blue
    set type_provider 0

    ;donne l'id du provider aux consumers
    ask consumers with [distance myself <= range_max] [
      set listProvider lput [who] of myself listProvider
    ]
  ]


  ;instancie les types de providers
  ask n-of round (nbProvider / 10) providers with [type_provider = 0][
    set type_provider "good"
  ]

  ask n-of round (nbProvider / 2.5) providers with [type_provider = 0][
    set type_provider "ordinary"
  ]

  ask n-of round (nbProvider / 2.22) providers with [type_provider = 0][
    set type_provider "intermittent"
  ]
  ask n-of round (nbProvider / 20) providers[
    set type_provider "bad"
  ]

  ;donne la moyenne du provider
  ask providers [
    giveMean
  ]


  set idTransaction (nbProvider + nbConsumer + 1)
end

to go

  ;pour random
  ask consumers with [length listProvider != 0][
    let idProvider one-of listProvider

    ;random selection
    makeTransaction idProvider
    ;supprime la transaction
    set ratingData remove-item (length ratingData - 1) ratingData

  ]

  set utility-RANDOM mean [utility] of consumers with [length listProvider != 0]

  ;pour IT
  ask consumers with [length listProvider != 0][
    let idProvider one-of listProvider

    ;si plusieur provider son dispo
    if length listProvider != 1 [
      set idProvider selectProvider
    ]

    makeTransaction idProvider

  ]
  set utility-IT mean [utility] of consumers with [length listProvider != 0]



  ;tue des agents
  ask n-of round (nbProvider / 50) consumers [
    die
  ]

  ;créé des agents
  create-consumers round (nbProvider / 50) [
    tilt-up asin (1.0 - random-float 2)
    jump range_circle

    set color red
    set ratingData []
    set listProvider []
    let temp []
    ask providers with [distance myself <= range_max] [
      set temp lput who temp
    ]
    set listProvider temp
  ]

  tick
end


;selectionne le provider
to-report selectProvider
  let trustValues []

  ;pour IT
  ;pour chaque provider du client
  foreach listProvider [
    idProvider ->
    let list_transaction getListTransaction-IT idProvider
    set trustValues lput (list (trustFunction list_transaction) idProvider) trustValues
  ]

  ;choix du provider en fonction de la temperature

  let list_trusted filter [ i -> item 0 i != 0] trustValues
  ;classe décroissant
  set list_trusted sort-by [ [a b] -> item 0 a >= item 0 b] list_trusted

  let list_not_trusted filter [ i -> item 0 i = 0] trustValues

  let err_profit exp ( (item 0 item 0 list_trusted) / (1 / ticks))
  let err_explore exp ( (utility_mean / 10) / (1 / ticks))

  let p_expl err_explore / ( err_explore + err_profit)



 ; err_profit  = exp( T_max / T(t) )
;    err_explore = exp( val(ug_mean()) / T(t))
;    p_expl = err_expl / (err_expl + err_profit)
;    if rand(p_expl) :
;        return r_choice(LNT)
;    else
;        return p_max



  ;selecte le provider
  let selected item 1 item 0 trustValues
  report selected

end

to-report trustFunction [list_transaction]

  let up 0
  let down 0
  let wieght 0

  let WK 2.0


  ;TK
  ifelse length list_transaction = 0 [
    report 0
  ][
    foreach list_transaction [
      transaction ->
      set wieght giveWieght-IT transaction
      set up (up + wieght * item 3 transaction)
      set down (down + wieght)
    ]
    let TK up / down

    ;PRK
    let gamma_k ( - ln 0.5)
    let PRK (1 - exp ( gamma_k * down))


    ;PDK
    set up 0
    set down 0
    set wieght 0

    foreach list_transaction [
      transaction ->
      set wieght giveWieght-IT transaction
      set down (down + wieght)
      set up (down * abs (item 3 transaction - TK))
    ]
    let PDK (1 - (0.5 * (up / down)))

    let PK PRK * PDK

    let omega_K WK * PK

    report (omega_K * TK) / omega_K

  ]
end

to-report giveWieght-IT [transaction]
  let time ticks - item 4 transaction
  let lambda (- (5 / ln 0.5))

  report exp ( - (time / lambda))
end


to giveMean
  let pl_perfect 10
  let pl_good 5
  let pl_ok 0
  let pl_bad -5
  let pl_worst -10

  ifelse type_provider = "good" [
    set mean_provider mean (list pl_perfect pl_good)
  ][
    ifelse type_provider = "ordinary" [
      set mean_provider mean (list pl_ok pl_good)
    ][
      ifelse type_provider = "bad" [
        set mean_provider pl_bad
      ][
        set mean_provider pl_ok
  ]]]
end


to-report getListTransaction-IT [id_provider]
  let listTransaction []
  ;pour chaque transaction de l'agent
  foreach ratingData [
    transaction ->
    ;si le provider est le même que celui actuellement
    if item 1 transaction = id_provider and item 2 transaction = service[
      ;on ajoute la transaction dans la liste
      set listTransaction lput transaction listTransaction
    ]
  ]
  report listTransaction
end


to makeTransaction [id_provider]

  giveUtility id_provider

  ;normalize l'utilité
  let note utility / 10

  ;fait la moyenne de toutes les transactions déja efféctué.
  set utility_sum utility + utility_sum
  set nbTransaction nbTransaction + 1
  set utility_mean utility_sum / nbTransaction


  let transaction (list idTransaction id_provider service note ticks)
  set idTransaction idTransaction + 1

  ;ajoute la transaction au consumer
  set ratingData lput transaction ratingData
  if length ratingData > 10 [
    set ratingData but-first ratingData
  ]
end

to giveUtility [id_provider]
  let distance_P 0
  let mean_P 0
  let type_P 0

  ;distance entre le consumer et le provider
  ask providers with [who = id_provider][
    set distance_P distance myself
    set mean_P mean_provider
    set type_P type_provider
  ]


  ifelse type_P = "good" [
    set utility random-normal mean_P 1.0
  ][
    ifelse type_P = "ordinary" [
      set utility random-normal mean_P 2.0
    ][
      ifelse type_P = "bad" [
        set utility random-normal mean_P 2.0
      ][
        ;random entre -5 et 5
        set utility (random-float 10) - 5
  ]]]

  ;dégradation linéaire de la distance
  let coeff (- 10 - utility) / (range_max - range_provider)

  if distance_P > range_provider [
    set utility utility + (coeff * (distance_P - range_provider))
  ]
  if distance_P = range_max [
    set utility -10
  ]

end

